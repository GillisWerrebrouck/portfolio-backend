{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "new-cap": ["error", {
      "newIsCap": true,
      "capIsNew": false
    }],
    "no-console": 0,
    "indent": ["error", 2, {
      "SwitchCase": 1,
      "MemberExpression": 1
    }],
    "no-use-before-define": ["error", { "functions": false, "classes": false }],
    "comma-dangle": ["error", {
    "arrays": "always-multiline",
    "objects": "always-multiline",
    "imports": "always-multiline",
    "exports": "always-multiline",
    "functions": "ignore"
    }],
    "linebreak-style": 0
  },
  "plugins": [
    "import"
  ]
}
